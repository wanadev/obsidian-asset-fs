        "obsidian-pack": "^1.0.1",
        "q": "^1.5.1",
        "uuid": "^3.2.1",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "browserify": "^16.2.0",
        "enlightme": "^0.1.1",
        "expect.js": "^0.3.1",
